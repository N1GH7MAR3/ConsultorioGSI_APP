<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_degradado"
    tools:context=".ProcedimientoPacienteActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id="@+id/customPrinciapl"
            layout="@layout/custom_tolbar_principal"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="171dp" />
        <TextView
            android:id="@+id/txtProcedimiento"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="PROCEDIMIENTOS"
            android:fontFamily="@font/roboto_medium"
            android:gravity="center"
            android:layout_marginTop="30dp"
            android:textSize="25dp"
            android:textColor="@color/gsiAzul"
            android:layout_marginBottom="20dp">
        </TextView>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvProcedimiento"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>